package studentINFObyusingINHERITANCE;

public class Trainer extends Person
{
private double salary;

public Trainer(int id, String name,double salary)
{
	super(id, name);
	this.salary=salary;
	
}

@Override
public String toString() {
	return "Trainer [salary=" + salary + ", toString()=" + super.toString() + ", getClass()=" + getClass()
			+ ", hashCode()=" + hashCode() + "]";
}


}
